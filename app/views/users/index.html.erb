<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Reg no</th>
      <th>Name</th>
      <th>Occupation</th>
      <th>Income</th>
      <th>Place of birth</th>
      <th>Dob</th>
      <th>Time of birth</th>
      <th>Brothers married</th>
      <th>Brothers unmarried</th>
      <th>Sister married</th>
      <th>Sister unmarried</th>
      <th>Gender</th>
      <th>Fathers name</th>
      <th>Residentail address</th>
      <th>Parents phone number</th>
      <th>Fathers occupation</th>
      <th>Fathers occupation address</th>
      <th>Fathers income</th>
      <th>Origin in sindh</th>
      <th>Height in inches</th>
      <th>Wight in kg</th>
      <th>Complexion</th>
      <th>Maritial status</th>
      <th>Physical status</th>
      <th>Remarks</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.reg_no %></td>
        <td><%= user.name %></td>
        <td><%= user.occupation %></td>
        <td><%= user.income %></td>
        <td><%= user.place_of_birth %></td>
        <td><%= user.dob %></td>
        <td><%= user.time_of_birth %></td>
        <td><%= user.brothers_married %></td>
        <td><%= user.brothers_unmarried %></td>
        <td><%= user.sister_married %></td>
        <td><%= user.sister_unmarried %></td>
        <td><%= user.gender %></td>
        <td><%= user.fathers_name %></td>
        <td><%= user.residentail_address %></td>
        <td><%= user.parents_phone_number %></td>
        <td><%= user.fathers_occupation %></td>
        <td><%= user.fathers_occupation_address %></td>
        <td><%= user.fathers_income %></td>
        <td><%= user.origin_in_sindh %></td>
        <td><%= user.height_in_inches %></td>
        <td><%= user.wight_in_kg %></td>
        <td><%= user.complexion %></td>
        <td><%= user.maritial_status %></td>
        <td><%= user.physical_status %></td>
        <td><%= user.remarks %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

